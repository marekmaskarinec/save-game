import (
        "window.um"; "rect.um"; "th.um"; "canvas.um"; "image.um";
        "../../tophat.dat/d_util.um"; "../../tophat.dat/d_pfx.um"
)

fn main() {
        window.setup("title", 640, 480)
        screen := rect.mk(0, 0, 640, 480)
        img := image.load(d_pfx.pfx()+"medium.png")

        for window.cycle(screen) {
                d_util.drawRectNinepatch(img, rect.Rect{0, 0, 24, 24}, rect.Rect{8, 8, 8, 8}, rect.Rect{220, 10, 200, 200})
                d_util.drawRectNinepatch(img, rect.Rect{24, 0, 24, 24}, rect.Rect{8, 8, 8, 8}, rect.Rect{10, 220, 200, 200})
                d_util.drawRectNinepatch(img, rect.Rect{24*2, 0, 24, 24}, rect.Rect{8, 8, 8, 8}, rect.Rect{10, 10, 200, 200})
        }
}