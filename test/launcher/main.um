import ("../../src/d_util.um"; "../../src/d_pfx.um")

fn main() {
  lf := d_util.readFileStr(d_pfx.pfx()+"local.txt")
  gf := d_util.readFileStr("data/global.txt")
  
  if lf != "I'm a local file!\n" {
    error("Invalid local file")
  } 
  
  if gf != "I'm a global file!\n" {
    error("Invalid global file")
  }

  printf("Ok\n")
}
